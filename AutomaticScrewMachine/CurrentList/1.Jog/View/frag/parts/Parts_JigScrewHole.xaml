<UserControl
    x:Class="AutomaticScrewMachine.CurrentList._1.Jog.View.frag.parts.Parts_JigScrewHole"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AutomaticScrewMachine.CurrentList._1.Jog.View.frag.parts"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="50"
    d:DesignWidth="80"
    DataContext="{Binding JogViewModel, Source={StaticResource Locator}}"
    mc:Ignorable="d">
    <Grid
        Grid.Column="1"
        Width="80"
        Background="Transparent">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10*" />
            <ColumnDefinition Width="2.5*" />
            <ColumnDefinition Width="10*" />
        </Grid.ColumnDefinitions>
        <Border
            Grid.ColumnSpan="3"
            Width="60"
            Background="DimGray"
            CornerRadius="10" />
        <Border
            Grid.Column="0"
            Width="15"
            Height="15"
            HorizontalAlignment="Right"
            Background="Black"
            CornerRadius="100" />
        <Border
            Grid.Column="2"
            Width="15"
            Height="15"
            HorizontalAlignment="Left"
            Background="Black"
            CornerRadius="100" />
    </Grid>
</UserControl>
